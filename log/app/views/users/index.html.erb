<h1>All Users</h1>
<table border="2px">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Passwords</th>
        </tr>
    </thead>
    <tbody>
        <% @user.each do |user| %>
            <tr>
                <td><%= user.name %></td>
                <td><%= user.email %></td>
                <td><%= user.password %></td>
            </tr>
        <% end %>
    </tbody>
</table>
